package cracteclasses;

public class Peletronico {
	String marca, objeto, sis;
	int lacamento, modelo, memoria;

	
	public Peletronico() {
		this.objeto= ("Celular");
	}
	
	
	public void status() {
		System.out.println("O "+this.objeto+" tem o sistema" + sis+" da marca "+ 
	this.marca+ "com memoria "+memoria+", modelo "+modelo+" e foi lançado no ano de "+this.lancamento+ ".");
		System.out.println("Seu sistema operacional é: "+sis+".");
	}

}
