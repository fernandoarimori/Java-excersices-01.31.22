package cracteclasses;

public class Patinete {
	String marca;
	double peso;
	int idade, idadem;
	
	
	
	
	public void status() {
		System.out.println("O patinete é da marca: "+this.marca);
		System.out.println("O peso máximo suportado é de: "+this.pose);
		System.out.println("O peso máximo suportado e mínimo é, respectivamente de: "+idadem+" e "+idade+".");
	}

}
